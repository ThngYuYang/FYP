@using System.Data
@model DataTable

<!DOCTYPE html>
<html>
<head>
   <meta name="viewport" content="width=device-width" />
   <title>Organic Fruits - Subscription List</title>
   <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
   <script src="~/lib/jquery/jquery.min.js"></script>
   <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
</head>
<body>
    @{
        // TODO: L05 Task 2e - Add the NavBar
        @await Html.PartialAsync("OrgNavBar")
    }

    <div class="container">
        <table class='table'>
            <tr>
                @for (int col = 0; col < Model.Columns.Count; col++)
                {
                    <th>@Model.Columns[col]</th>
                }
            </tr>

            @for (int row = 0; row < Model.Rows.Count; row++)
            {
                <tr>
                    @for (int col = 0; col < Model.Columns.Count; col++)
                    {
                        <td>@Model.Rows[row][col]</td>
                    }
                </tr>
            }
        </table>
    </div>


</body>
</html>
